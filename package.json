{
  "name": "@nebulae-cli/deploy",
  "version": "0.1.0",
  "description": "Command line tools for deploying",
  "private": false,
  "type": "module",
  "main": "./esm/index.js",
  "bin": {
    "deploy": "./esm/index.js"
  },
  "files": [
    "esm",
    "templates",
    "package.json"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.esm.json",
    "deploy:dev": "node --import ./ts-node.register.js src/index.ts",
    "lint": "eslint . --fix",
    "pub": "npm run build && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/longlongago2/deploy-cli.git"
  },
  "keywords": [
    "deploy",
    "cli",
    "node"
  ],
  "author": "longlongago2",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/longlongago2/deploy-cli/issues"
  },
  "homepage": "https://github.com/longlongago2/deploy-cli#readme",
  "dependencies": {
    "archiver": "^7.0.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "js-yaml": "^4.1.0",
    "ora": "^8.0.1",
    "readline-sync": "^1.4.10",
    "slash": "^5.1.0",
    "ssh2": "^1.15.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@types/archiver": "^6.0.2",
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.14.9",
    "@types/readline-sync": "^1.4.8",
    "@types/ssh2": "^1.15.0",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.8.0",
    "prettier": "3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.15.0"
  },
  "overrides": {
    "eslint": "^9.6.0"
  },
  "engines": {
    "node": ">=18"
  }
}
